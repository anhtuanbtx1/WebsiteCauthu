<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewData["Title"] - Home</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    @if (IsSectionDefined("Styles"))
    {@RenderSection("Styles", required: false)}

    <environment names="Development,Staging,Production">
        <link id="skin-default" rel="stylesheet" href="~/lib/fontawesome-free/css/all.min.css">
        <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
        <link rel="stylesheet" href="~/lib/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
        <link id="skin-default" rel="stylesheet" href="~/lib/icheck-bootstrap/icheck-bootstrap.min.css">
        <link rel="stylesheet" href="~/css/adminlte.min.css">
        <link id="skin-default" rel="stylesheet" href="~/lib/overlayScrollbars/css/OverlayScrollbars.min.css">
        <link rel="stylesheet" href="~/lib/daterangepicker/daterangepicker.css">
        <link rel="stylesheet" href="~/lib/summernote/summernote-bs4.css">
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700">
    </environment>

</head>
<body class="hold-transition sidebar-mini layout-fixed layout-navbar-fixed layout-footer-fixed">
    <div class="wrapper">

        <partial name="_Navbar" />

        <partial name="_SideBar" />
        @RenderBody()
        <partial name="_Footer" />

    </div>
    <!-- ./wrapper -->

    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/lib/chart.js/Chart.min.js"></script>
    <script src="~/lib/sparklines/sparkline.js"></script>
    <script src="~/lib/jquery-knob/jquery.knob.min.js"></script>
    <script src="~/lib/moment/moment.min.js"></script>
    <script src="~/lib/daterangepicker/daterangepicker.js"></script>
    <script src="~/lib/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <script src="~/lib/summernote/summernote-bs4.min.js"></script>
    <script src="~/lib/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <script src="~/js/adminlte.js"></script>
    <script src="~/js/demo.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
        }